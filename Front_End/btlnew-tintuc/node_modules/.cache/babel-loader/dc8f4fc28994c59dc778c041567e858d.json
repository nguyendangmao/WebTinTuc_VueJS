y=\"nhomtin.idNhomTin\"\r\n                >\r\n                  <router-link :to=\"{ name:'NewsByGenre',params: {id:nhomtin.idNhomTin}}\" class=\"nav-item nav-link \" >{{nhomtin.tenNhomTin}}</router-link >\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!-- News With Sidebar End -->\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      nhomtins: [],\r\n      baiviets: [],\r\n      quangcao: {},\r\n    };\r\n  },\r\n  created() {\r\n    this.getnhomtins();\r\n    this.getbaiviets();\r\n    this.LayAnhQuangCao();\r\n  },\r\n  props: [\"BaiViet\"],\r\n  methods: {\r\n    getnhomtins() {\r\n      this.$request\r\n        .get(\"https://localhost:44309/api/NhomTinAPI\")\r\n        .then((res) => {\r\n          this.nhomtins = res.data;\r\n        });\r\n    },\r\n    getbaiviets() {\r\n      this.$request.get(\"https://localhost:44309/api/BaiViet\").then((res) => {\r\n        this.baiviets = res.data;\r\n      });\r\n    },\r\n    LayAnhQuangCao() {\r\n      axios.get(\"https://localhost:44309/api/QuangCao/1\").then((response) => {\r\n        this.quangcao = response.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n@import url(../../src/assets/css/style.css);\r\n</style>"]},"metadata":{},"sourceType":"module"}